      "cell_type": "code",
      "source": [
        "def weighted_mae(y_true, y_pred, is_holiday):\n",
        "    weights = np.where(is_holiday, 5, 1)\n",
        "    return np.round(np.sum(weights * np.abs(y_true - y_pred)) / np.sum(weights), 4)"
      ],
      "metadata": {
        "id": "GsL1SoCUzXA4"
      },
      "execution_count": 23,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Step 10. Evaluate with WMAE\n",
        "# We need to get IsHoliday values for the validation rows\n",
        "val_idx = X_val.index\n",
        "is_holiday_val = train.loc[val_idx, \"IsHoliday\"]\n",
        "\n",
        "y_pred = model.predict(X_val)\n",
        "wmae = weighted_mae(y_val, y_pred, is_holiday_val)\n",
        "print(\"Validation WMAE:\", wmae)\n"
      ],
